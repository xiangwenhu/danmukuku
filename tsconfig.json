{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es5",
        "moduleResolution": "node",
        "jsx":"preserve",
        "allowJs": true,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "allowUmdGlobalAccess": true,
        "lib": [
            "dom",
            "es5",
            "es2015",
            "es2016"
        ]
    },
    "include": ["./lib"]
}